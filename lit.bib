@Book{Marcos2000,
  author     = {Natalio {Fernández Marcos}},
  title      = {The Septuagint in Context: Introduction to the Greek Version of the Bible},
  location   = {Leiden and Boston},
  publisher  = {Brill},
  year       = {2000},
  shorttitle = {The Septuagint in Context},
  abstract   = {This translation of the second revised and expanded Spanish edition deals fully with the origins of the Septuagint. It discusses its linguistic and cultural frame, its relation to the Hebrew text and to the Qumran documents, the transmission of the Septuagint and its reception by Jews and Christians. It includes the early revisions, Aquila, Symmachus and Theodotion, the Christian recensions and particularly Origen's Hexapla, Biblical commentaries and catenae, as well as other issues such as the relation of the Septuagint to Hellenism, to the New Testament and to Early Christian Literature.
It is a comprehensive introduction to the Septuagint, the first translation and interpretation of the Hebrew Bible, and to other Greek versions of the Bible.},
