@Article{Schenker2010,
  author       = {Adrian Schenker},
  title        = {Man bittet um das Gegenargument! Von der Eigenart textkritischer Argumentation},
  journaltitle = {ZAW},
  volume       = {122},
  pages        = {53-63},
  year         = {2010},
  shorttitle   = {Man bittet um das Gegenargument},
  abstract     = {Der Artikel ist eine Reflexion über die Eigenart der Argumentation in der Textkritik. Gegen die meisten textkritischen Argumente lässt sich ein Gegenargument geltend machen, weil Textdifferenzen in der Regel auf zwei entgegengesetzte Weisen erklärt werden können. Daher besteht Textkritik im Abwägen von Argumenten. Die Darlegung eines Arguments allein reicht nicht. Diese Methode wird an drei in der gegenwärtigen Forschung umstrittenen Textdifferenzen illustriert (I Reg 11 f.; 14; I Reg 12,31 mit II Reg 17,29.32; II Reg 23,13) sowie an einer vierten, die überraschenderweise nicht umstritten ist, aber es sein sollte (Jer 31,33), ein Beispiel, das einen anderen gelegentlichen Mangel textkritischer Arbeiten zeigt: Textdifferenzen werden übersehen, wenn ein Text keine offensichtliche Schwierigkeit bereitet.},
  file         = {Volltext:S/Schenker2010.pdf:PDF},
  keywords     = {Textkritik, LXX, Kön, Reg, Jer},
  timestamp    = {2010.05.02},
}

