@Article{Schipper2009,
  author       = {Bernd U. Schipper},
  title        = {Die »eherne Schlange«. Zur Religionsgeschichte und Theologie von Num 21,4–9},
  journaltitle = {ZAW},
  volume       = {121},
  pages        = {369-387},
  year         = {2009},
  shorttitle   = {Die »eherne Schlange«},
  abstract     = {Vor dem Hintergrund altorientalischen Materials erweist sich die Heilung durch den Blick in Num 21,4–9 als völlig singulär. Sie steht im Gegensatz zu allem, was über Schlangenbeschwörung im Alten Orient bekannt ist, da bei erfolgtem Schlangenbiss immer Rituale (teilweise in Verbindung mit Schlangenappliken) zum Einsatz kamen. Die Erzählung von der ehernen Schlange ist vielmehr von Dtn 18 und Ex 15,26 her zu lesen und steht im Kontext einer Tradition, die sich dezidiert gegen jede Form von Magie und Ritual ausspricht und den »Mittler« Mose an die Stelle des religiösen Spezialisten setzt. Num 21,4–9 ist demnach ein später Text, der vermutlich auf die nach wie vor bestehende Praxis von Schlangenbeschwörung und magisch-rituellen Praktiken reagiert und der anhand einer Beispielgeschichte aus der Wüstenzeit diesen entgegenwirken will.},
  file         = {Volltext:S/Schipper2009.pdf:PDF},
  timestamp    = {2010.05.02},
}

