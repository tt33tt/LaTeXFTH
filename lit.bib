@Article{Louw2008,
  author       = {Theo A.W. {van der Louw}},
  title        = {The Dictation of the Septuagint Version},
  journaltitle = {JSJ},
  volume       = {39},
  pages        = {211-229},
  year         = {2008},
  abstract     = {In Antiquity, original writing, copying and translating took place through dictation. It is likely that (parts of) the Septuagint was (were) committed to writing in that way. Traditions concerning the translation of Buddhist Sutras into Chinese help us to picture that process. The hypothesis that someone recited the Hebrew text, one translated orally and one or more scribes wrote the translation down contributes to LXX research. It explains (1) characteristic features of the Septuagint noted by Soisalon-Soininen, and (2) phonetic errors on both the Hebrew and the Greek sides. (3) It is compatible with the notion that learned scribes were involved but it lends no support to the "targumic origin" theory.},
  file         = {Volltext:L/Louw2008.pdf:PDF},
  keywords     = {SEPTUAGINT; DICTATION; ANTIQUITY; TRANSLATING; WRITING; COPYING; INTERPRETING; PHONETIC ERRORS; DICTATION, SEPTUAGINT, BUDDHIST SUTRAS},
  sortname     = {LouwTheoAWTheDictation},
  timestamp    = {2009.12.10},
}

@Book{Louw2007,
  title      = {Transformations in the Septuagint: Towards an Interaction of Septuagint Studies and Translation Studies},
  publisher  = {Peeters},
  year       = {2007},
  author     = {Theo A.W. {van der Louw}},
  number     = {47},
  series     = {CBET},
  comment    = {Die von Arie van der Kooij betreute Dissertation (Leiden 2006) bemüht sich darum, eine größere methodische Klarheit in die exegetische Beschäftigung mit der LXX zu bringen. Dazu sollen Kriterien, die in der modernen Übersetzungswissenschaft entwickelt wurden, in die LXX-Forschung übernommen werden, um auf diese Weise die zu beobachtenden Phänomene genauer bewerten und klassifizieren zu können. In einem knappen Einführungskapitel »State of the Question and Purpose of this Study« (S. 1–23) wird der Diskussionsstand hinsichtlich der interdisziplinären Berührung zwischen LXX- und Übersetzungsforschung dargestellt. Danach gibt Kap. 2 einen sehr lesenswerten Überblick über »Translating and Translations in Antiquity« (S. 25–55), der sich v. a. auf das Übersetzen ins Griechische und die wenigen jüdischen Stimmen aus der Antike zum Problem konzentriert. Die eigentliche Methodik wird in Kap. 3 »Inventory of Transformations« (S. 57–92) dargelegt, das die möglichen Veränderungen zusammenstellt, die im Vorgang des Übersetzens geschehen können. Von da aus wird S. 90 f. ein modellhaftes Vorgehen für den Umgang mit Vergleichen zwischen LXX und Vorlage vorgestellt, dem dann im weiteren Verlauf gefolgt wird. Dieses Modell weicht deutlich von dem ab, was derzeit im angelsächsischen Raum unter den Namen »Interlinearitäts-Paradigma« diskutiert wird, und es ist bedauerlich, daß der Vf. sich nur äußerst knapp mit diesem wichtigen theoretischen Gegenentwurf auseinandersetzt (S. 154). Sehr ausführlich werden dann die Transformationsphänomene in den genannten Kapiteln Gen 2 (S. 93–154); Jes 1 (155–247) und Prov 6 (249–356) dargestellt und am Ende der Abschnitte bewertet und tabellarisch geordnet. Die Ergebnisse sind nicht prinzipiell neu oder umstürzend, und die Erklärungen im einzelnen sind unterschiedlich plausibel. Aber die verwendete Klassifizierung ermöglicht eine sehr hohe Transparenz der Argumentation, die überhaupt durch ihre Genauigkeit beeindruckt. Ein abschließendes, mit »Conclusions« überschriebenes Kapitel (S. 357–373) überträgt die Einzelbeobachtungen der Musterexegesen auf eine allgemeine Ebene und vergleicht die Übersetzungsweisen untereinander; es wird aber durch die Neueröffnung exegetischer Diskussionen zu Gen 1 und Jes 36 etwas unübersichtlich. Eine hilfreiche Übersicht über Fachtermini und spezielle Begriffsdefinitionen, eine Bibliographie und die üblichen Register beschließen die wichtige Studie. (M. Rösel, Rostock; ZAW 121(2008), 162)},
  file       = {Auszug:http\://books.google.de/books?id=F6DHbSnDnwoC&lpg=PR7&dq=intitle%3Atransformations%20inauthor%3Alouw&lr&as_drrb_is=q&as_minm_is=0&as_miny_is&as_maxm_is=0&as_maxy_is&as_brr=0&pg=PR1#v=onepage&q&f=false:URL;Rezension:L/vanderLouw_Transformations_Rez.pdf:PDF},
  location   = {Leuven and Paris and Dudley},
  shorttitle = {Transformations in the Septuagint},
  sortname   = {LouwTheoAWTransformations2007},
  timestamp  = {2010.05.02},
}

